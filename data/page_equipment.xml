<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<!-- split between ir_ui_view & website_page record to keep external_id on ir_ui_view -->
		<record id="page_add_equipment_view" model="ir.ui.view">
			<field name="name">add_equipment</field>
			<field name="type">qweb</field>
			<field name="key">website.add_equipment</field>
			<field name="arch" type="xml">
				<t name="add_equipment" priority="60" t-name="website.add_equipment">
					<t t-call="website.layout">
						<!--<t t-set="pageName" t-value="'add_equipment'"/>-->
						<div id="wrap">
							<div id="page-top"/>
							<div class="oe_structure">
								


							</div>
						</div>
					</t>
				</t>
			</field>
		</record>

		<record id="page_add_equipment_page" model="website.page">
			<field name="url">/equipment</field>
			<field name="is_published">True</field>
			<field name="view_id" ref="page_add_equipment_view" />
			<field name="is_homepage">False</field>
		</record>
		<record id="menu_add_equipment" model="website.menu">
			<field name="name">Доп. оборудование</field>
			<field name="url">/equipment</field>
			<field name="parent_id" ref="website.main_menu" />
			<field name="sequence" type="int">50</field>
			<field name="page_id" ref="page_add_equipment_page" />
			<!-- <field name="website_id" ref="website.default_website" /> -->
		</record>
	</data>
</odoo>
